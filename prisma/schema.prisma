  file_name          String
  file_path          String
  rule_id            String?
  record_id          String
  task_id            String
  group_id           String?
  mass_context_id    String?
  mass_type          String?
  mass_sub_type      String?
  attribute_missing  String?
  attribute_mismatch String?
  additional_fields  String?
  additional_data    String?
  status             String?
  creation_date      String
  update_date        String
  application_id     String

  @@index([group_id], map: "uplift_files_processing_1ix")
  @@index([status], map: "uplift_files_processing_2ix")
  @@ignore
}

model uplift_group_info {
  group_id                  String
  domain                    String
  mnpcod                    String?
  group_criteria_identifier String?
  bucket_status             String
  num_of_tickets            Int
  creation_date             DateTime  @db.Timestamp(6)
  ctcr_sys_upd_dt           DateTime? @db.Timestamp(6)

  @@id([group_id, domain], map: "uplift_bucket_info_pk")
  @@index([domain], map: "uplift_bucket_info_ix1")
  @@index([group_id], map: "uplift_bucket_info_trc_ix1")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model uplift_population_list {
  id   String
  type String

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model uplift_population_list_dfs {
  id   String?
  type String?

  @@ignore
}

model uplift_recon_rules {
  rule_id     Int     @id
  rule_name   String
  domain      String
  alias       String
  rule_type   String
  description String?
}

model uplift_rules {
  rule_id     Int     @id
  rule_name   String
  domain      String
  alias       String
  rule_type   String
  description String?
}

model user_disposition {
  user_id          String
  disposition_name String
  event_time       DateTime @db.Timestamp(6)

  @@id([user_id, event_time], map: "user_diposition_pk")
}

model user_preferences {
  user_id                  String
  pref_key                 String
  sub_pref_key             String
  pref_value               String?
  ctrl_data_last_oper_id   String?
  ctrl_data_last_oper_date DateTime? @db.Timestamp(6)

  @@id([user_id, pref_key, sub_pref_key], map: "user_preferences_pk")
}

model user_preferences_active {
  user_id                  String
  pref_key                 String
  sub_pref_key             String
  ctrl_data_last_oper_id   String?
  ctrl_data_last_oper_date DateTime? @db.Timestamp(6)

  @@id([user_id, pref_key, sub_pref_key], map: "user_preferences_active_pk")
}

model ws_applauth {
  appl_code                String
  activity_code            String
  security_profile_code    String
  activity_mode            String?
  ctrl_data_last_oper_id   String?
  ctrl_data_last_oper_date DateTime? @db.Timestamp(6)

  @@id([appl_code, activity_code, security_profile_code], map: "ws_applauth_pk")
}

model api_keys {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  key         String   @unique @default(dbgenerated("('dandy-'::text || gen_random_uuid())"))
  name        String
  description String?
  created_at  DateTime @default(now()) @db.Timestamptz(6)
  updated_at  DateTime @default(now()) @db.Timestamptz(6)

  @@index([key], map: "idx_key")
}

enum applied_billing_rate_type {
  oneTimeCharge
  recurringCharge
  usageCharge
  discountCharge
  credit
}
